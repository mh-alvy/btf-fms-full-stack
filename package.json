{
  "name": "btf-fms-fullstack",
  "version": "1.0.0",
  "description": "Break The Fear Fee Management System - Full Stack Application",
  "scripts": {
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "install:all": "npm run install:backend && npm run install:frontend",
    "build:frontend": "cd frontend && npm run build",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm start",
    "start:dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start": "node start-production.js"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "concurrently": "^7.6.0"
  },
  "author": "Break The Fear",
  "license": "MIT"
}
